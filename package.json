{
  "name": "npm-search-scoped-packages",
  "version": "1.0.0",
  "description": "Searches NPM Packages with a given scope",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "clean": "ts-clean -e .d.ts,.js,.js.map",
    "clean-build": "ts-clean -e .d.ts,.js,.js.map && tsc",
    "watch": "tsc -w"
  },
  "keywords": [
    "npm",
    "search",
    "scope",
    "package"
  ],
  "author": "jforge",
  "license": "MIT-0",
  "devDependencies": {
    "@types/node": "^13.7.0",
    "@types/node-fetch": "^2.5.4",
    "ts-clean": "^1.0.3",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5"
  },
  "dependencies": {
    "axios": "^0.19.2"
  }
}
